<%= simple_form_for @extraction do |f| %>
  <%= f.error_notification %>
    <div class="row">
      <div class="form-group col-xs-5 col-md-5">
        <%= f.input :search_id, :collection => ["-", "1","2", "3"], :selected => "-" %>
      </div>
      <div class="form-group col-xs-5 col-md-5">
        <%= f.input :template_id, :collection => @templates_name, :selected => "-" %>
      </div>
      <div class="col-xs-2 col-md-2 vcenter">
        <button type="submit" class="btn btn-primary">Generate</button>
      </div>
    </div>
<% end %>


